page{
  background: #F9F9F9;
}

.viewBox{
  height: 100%;
  width: 702rpx;
  margin: 22rpx 42rpx;
  border-radius:22rpx 22rpx 0 0 ;
  position: relative;

}

.videoimg{
  width: 100%;
  height: 396rpx;
}

.videobutton{
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  top: 33%;
  left: 50%;
  transform: translate(-50%,-50%)
}
.videotitle{
  font-size: 42rpx;
  font-family: PingFang-SC-Bold;
  color: #292929;
  font-weight: bold;
  padding: 30rpx 40rpx 0 30rpx;
}
.videosubtext{
  color: #A3A3A3;
  font-size: 32rpx;
  padding: 0px 40rpx 0 30rpx;
}