 /*显示隐藏*/
 .p-flex { display: flex;}
 .p-hide { display:none; }
 .p-block { display:block; }
 .p-inline { display:inline; }
 .p-table-cell { display: table-cell}
 .p-inline-flex {display: inline-flex}
 .p-inline-block { display:inline-block; }
 .p-v-h { visibility:hidden; }
 .p-v-v { visibility:visible; }
 .p-flex-1{flex:1}

 /* 垂直居中 */
 .p-vertical-align { vertical-align: middle; }

 /* 居中对齐弹性盒 */
 .p-al-item{align-items:center;}

 /*浮动*/
 .p-left{ float:left; }
